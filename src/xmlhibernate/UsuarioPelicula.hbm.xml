<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="">

	<class name="UsuarioPelicula" table="usuario_pelicula">

		<composite-id
        	name="usuarioPeliculaId"
        	class="UsuarioPeliculaId">

        	<key-property name="idusuario" type="java.lang.Integer" column="idusuario" />
        	<key-property name="id" type="java.lang.String" column="id" />
        	
		</composite-id>

	</class>
</hibernate-mapping>
